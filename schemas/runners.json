    "type": "object",
    "additionalProperties": false,
    "properties": {
        "name1": {
            "type": "string",
            "description": "Name of first runner",
            "default": ""
        },
        "name2": {
            "type": "string",
            "description": "Name of second runner",
            "default": ""
        },
        "name3": {
            "type": "string",
            "description": "Name of third runner",
            "default": ""
        },
        "name4": {
            "type": "string",
            "description": "Name of fourth runner",
            "default": ""

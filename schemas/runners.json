        "time1": {
            "type": "string",
            "description": "Time of first runner",
            "default": ""
        },
        "time2": {
            "type": "string",
            "description": "Time of second runner",
            "default": ""
        },
        "time3": {
            "type": "string",
            "description": "Time of third runner",
            "default": ""
